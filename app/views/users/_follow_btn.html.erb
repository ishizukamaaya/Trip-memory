<!--ログインuserがuserでない時-->
  <% if current_user != user %>
    <!--ログインuserはfollow?-->
    <% if current_user.following?(user) %>
      <%= link_to "フォローを外す", user_relationship_path(user.id), method: :delete, class: "btn btn-primary fab fa-twitter", remote: true %>
    <% else %>
      <%= link_to 'フォローする', user_relationships_path(user.id), method: :POST, class: "btn btn-success fab fa-twitter", remote: true %>
    <% end %>
  <% end %>